{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://rivalis-backend.railway.app/api"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://rivalis-backend.railway.app/api"
    }
  }
}